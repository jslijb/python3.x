<script>
//打开计算器
function calculator(){
	var amount = parseInt($("#tjine").val()) || 0;							//投资金额
	var deadline  = parseInt($("#qiCi").val()) || 0;						//投资期限
	var dataType = parseInt($("#calculate .mninput").attr("data-val"));		//投资期限（天/月）
	var rate = GetInterestRate(dataType,parseFloat($("#liLv").val())) || 0;	//利率
	var loanType = $("#fangShi").find("span.active").data("val");			//还款方式
	var lixi = 0;

	if(loanType == 2) {
		$.each(ECAI(amount,deadline,rate), function(i,v) {
			lixi += v.ChangHaiLiXi;
		});
	} else {
		lixi = amount * deadline * rate;
	}
	benxi = amount + lixi;
	$("#benXi").text(ForDight(benxi, 2));
	$("#liXi").text(ForDight(lixi, 2));
}
function ECAI(jine,qici,lilv) {				//等额本息列表
	var benxi = jine * lilv * Math.pow(lilv + 1, qici) / (Math.pow(lilv + 1, qici) - 1);
	var shengyubenjin = jine;

	var feeInfos = [];
	for(var i=1; i<= qici; i++) {
		var lixi = shengyubenjin * lilv;
		var benjin = benxi - lixi;
		shengyubenjin -= benjin;
		var feeInfo = {
			Qici : i,
			ChangHaiBenJin : benjin,
			ChangHaiBenXi : benxi,
			ChangHaiLiXi : lixi,
			ShengYuBenJin : shengyubenjin
		};
		feeInfos.push(feeInfo);
	}
	return feeInfos;
}

function GetInterestRate(dateType, rate) {		//根据借款周期单位和利率换算实际利率
	var interestRate = 0;
	if (dateType == 2) {
		interestRate = rate / 1200;
	} else if (dateType == 1) {
		interestRate = rate / 36000;
	}
	return interestRate;
}
function ForDight(Dight, How) {
	Dight = Math.round(Dight * Math.pow(10, How)) / Math.pow(10, How);
	return Dight;
}
//选择还款类型
$("#calculate .type").on('click','span',function(){
	var i = $(this).parents("dd").find('i');
	$("#calculate .type span").removeClass("active");
	$(this).addClass("active");
	i.appendTo($(this));
	if($(this).data('val') != 1) {
		$(".mnSelect li:last").click();
		$(".mninput").off("click").css('background-color','#eee');
	} else {
		$(".mninput").on('click',mninput).css('background-color','#fff');
	}
})
//模拟下拉
$("#calculate .mninput").on('click',mninput);
function mninput(){
	var obj = $(this).data("mname");
	$("."+obj).show().children("li").slideDown('fast');
}
$("#calculate .mnSelect li").on("click",function(){
	var obj = $(this).data("target");
	var val = $(this).data("val");
	var str = $(this).text();
	$("."+obj).attr("data-val",val).html(str+'<i class="iconfont mnDown">&#xe63a;</i>');
	$(".mnSelect").hide().children("li").slideUp();
});

$(".ibd-tools a").hover(function(){
	var width = $(this).find(".toggleInfo").data('width');
	$(this).find(".toggleInfo").show().animate({width:width},100)
},function(){
	$(this).find(".toggleInfo").hide().animate({width:0},100)
})
//限制输入数字
$("[onlyNum]").onlyNum();

//回到顶部
$(window).on("scroll.goTop",function(){
	if($(document).scrollTop() >= 600){
		$(".goTop").fadeIn('fast');
	} else if($(document).scrollTop() < $(window).height() * 0.6){
		$(".goTop").fadeOut('slow');
	}
});
$(".goTop").hover(function(){
	$(this).find("span").toggle();
});
</script>
