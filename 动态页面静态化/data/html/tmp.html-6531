//预计收益
var _creditType =1;//取还款方式
var _bidMoney =3000000;	//融资金额
var _deadline =7;	//时长
var _dateType =1;	//时间单位
var _creditMode =1;  //标的类型
var _isShow = 0;	//是否显示加息
var _raiseRate = 0;	//加息利率
var _raiseAmount = 0;//加息额度
function showValue(){
	var obj = this.value;
    var _creditUpperLimit = parseFloat(3000000);	//最大投标额度
	var _creditDifferenceOfWindow = parseFloat(3000000-3000000);	//剩余可加入金额
	if(!isNaN(obj)){
		//利率
		var _creditRate = 8;
		if(_isShow > 0 && obj >= _raiseAmount){
			_creditRate += _raiseRate;
		}
		_creditRate /= 100;
		var yuji = 0;
		if(_dateType == 1){
			yuji = obj * _creditRate /360 * _deadline;
		}else{
			//等额本息
			if(_dateType == 2 && _creditType == 2){
				yuji = Math.pow(1+_creditRate/12, _deadline) * _deadline;
				for(var i=0; i<_deadline; i++){
					yuji -= Math.pow(1+_creditRate/12, i);
				}
				yuji *= obj * _creditRate/12 / (Math.pow(1+_creditRate/12, _deadline) - 1);
			} else {
				yuji = obj * _creditRate /360 * _deadline * 30;
			}
		}
		$(".yuji i").text(numcode(yuji));
	}
	return false;
}
