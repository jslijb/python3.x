	var v;
	
	$(".do button").attr("class","onibd").prop("disabled",false).html("立即投资");
	var ze = (3000000*100-3000000*100)/100;	//剩余可加入金额
	
	obj.on("blur","input",function(){
		v = $(this).val();	//获取值
		$(this).removeClass("allTipbg").removeAttr("tip");pass = true;	//初始化
	}).on("blur","[jine]",function(){
		if(!!v && !/^\+?(?!0+(\.00?)?$)\d+(\.\d\d?)?$/.test(v)){
			allTips($(this).attr("tip","无效的数值。"),"无效的数值。");
			pass = false;
		} else if(!login) {
			allTips($(this).attr("tip","登录已失效，请重新登录。"),"登录已失效，请重新登录。");
			pass = false;
		} else if(v > userBalance) {
			allTips($(this).attr("tip","投资金额大于你的可用余额。"),"投资金额大于你的可用余额。");
			pass = false;
		} else if(v > 3000000) {
			allTips($(this).attr("tip","投资金额超出最大投资额度。"),"投资金额超出最大投资额度。");
			pass = false;
		} else if(ze > 100 && v < 100) {
			allTips($(this).attr("tip","最小购买金额是100元。"),"最小购买金额是100元。");
			pass = false;
		} else if(ze <= 100 && v != ze) {
			allTips($(this).attr("tip","最小购买金额是" + ze + "元。"),"最小购买金额是" + ze + "元。");
			pass = false;
		} else if(v > ze) {
			allTips($(this).attr("tip","投资金额大于剩余可加入金额。"),"投资金额大于剩余可加入金额。");
			pass = false;
		}
	})
	obj.on("click","[submit]",function(event){
